  m_Script: {fileID: 11500000, guid: 524271193aa441aa84eba3efd64f23d5, type: 3}
  m_Name: so_Dialogue_ID10Dia2Tutorial_Amai
  m_EditorClassIdentifier: 
  dialogueName: so_Dialogue_ID10Dia2Tutorial_Amai
  dialogueId: 10
  dialogueContent:
  - speakerId: character_Amai
    speakerName: Amai
    sentence: Bom dia, filha! Dormiu bem?
    icon: {fileID: 21300000, guid: 490294f374b60b34881d28e9364b1608, type: 3}
    audio: {fileID: 0}
  - speakerId: character_Amai
    speakerName: Amai
    sentence: "Desculpa n\xE3o te acompanhar hoje na primeira refei\xE7\xE3o. Acabei
      ocupada com a organiza\xE7\xE3o do festival para a Grande M\xE3e."
    icon: {fileID: 21300000, guid: 490294f374b60b34881d28e9364b1608, type: 3}
    audio: {fileID: 0}
  - speakerId: character_Niara
    speakerName: Niara
    sentence: "Bom dia, m\xE3e. Dormi bem sim, e a senhora?"
    icon: {fileID: 21300000, guid: c6485e1d61aedaf4a9475bf958b1f17a, type: 3}
    audio: {fileID: 0}
  - speakerId: character_Niara
    speakerName: Niara
    sentence: "N\xE3o se preocupe em rela\xE7\xE3o a primeira refei\xE7\xE3o! Ainda
      teremos muitas juntas."
    icon: {fileID: 21300000, guid: c6485e1d61aedaf4a9475bf958b1f17a, type: 3}
    audio: {fileID: 0}
  - speakerId: character_Amai
    speakerName: Amai
    sentence: "\xC9 verdade, querida! Ainda vamos ter muito tempo juntas."
    icon: {fileID: 21300000, guid: 490294f374b60b34881d28e9364b1608, type: 3}
    audio: {fileID: 0}
  - speakerId: character_Amai
    speakerName: Amai
    sentence: "Ah! Quase me esqueci! O anci\xE3o Daktari queria falar com voc\xEA
      e est\xE1 te aguardando..."
    icon: {fileID: 21300000, guid: 490294f374b60b34881d28e9364b1608, type: 3}
    audio: {fileID: 0}
  - speakerId: character_Amai
    speakerName: Amai
    sentence: "Assim que terminar de falar com ele, venha me encontrar no Templo
      da Deusa. Estarei l\xE1 ajudando na organiza\xE7\xE3o do festival."
    icon: {fileID: 21300000, guid: 490294f374b60b34881d28e9364b1608, type: 3}
    audio: {fileID: 0}
  - speakerId: character_Niara
    speakerName: Niara
    sentence: "Claro, m\xE3e. Vou conversar com ele e j\xE1 j\xE1 te encontro para
      ajudar na organiza\xE7\xE3o."
    icon: {fileID: 21300000, guid: c6485e1d61aedaf4a9475bf958b1f17a, type: 3}
    audio: {fileID: 0}
