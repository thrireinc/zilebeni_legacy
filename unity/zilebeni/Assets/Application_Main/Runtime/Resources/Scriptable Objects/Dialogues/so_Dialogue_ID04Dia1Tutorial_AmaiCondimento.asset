  m_Script: {fileID: 11500000, guid: 524271193aa441aa84eba3efd64f23d5, type: 3}
  m_Name: so_Dialogue_ID04Dia1Tutorial_AmaiCondimento
  m_EditorClassIdentifier: 
  dialogueName: so_Dialogue_ID04Dia1Tutorial_AmaiCondimento
  dialogueId: 4
  dialogueContent:
  - speakerId: character_Amai
    speakerName: Amai
    sentence: "Filha! Como voc\xEA est\xE1?"
    icon: {fileID: 21300000, guid: 490294f374b60b34881d28e9364b1608, type: 3}
    audio: {fileID: 0}
  - speakerId: character_Amai
    speakerName: Amai
    sentence: "Mal te vi essa manh\xE3 antes de voc\xEA ir se encontrar com a general
      Luena."
    icon: {fileID: 21300000, guid: 490294f374b60b34881d28e9364b1608, type: 3}
    audio: {fileID: 0}
  - speakerId: character_Niara
    speakerName: Niara
    sentence: "Oi, m\xE3e! Desculpa n\xE3o ter ido ver a senhora antes do treino.
      Mas estava ansiosa... achei que finalmente ir\xEDamos passar para o ataque. "
    icon: {fileID: 21300000, guid: c6485e1d61aedaf4a9475bf958b1f17a, type: 3}
    audio: {fileID: 0}
  - speakerId: character_Niara
    speakerName: Niara
    sentence: Mas continuamos na defesa.
    icon: {fileID: 21300000, guid: c6485e1d61aedaf4a9475bf958b1f17a, type: 3}
    audio: {fileID: 0}
  - speakerId: character_Amai
    speakerName: Amai
    sentence: "Quer dizer que a poderosa e r\xEDgida general n\xE3o quer treinar
      minha beb\xEA na arte de atacar o inimigo?"
    icon: {fileID: 21300000, guid: 490294f374b60b34881d28e9364b1608, type: 3}
    audio: {fileID: 0}
  - speakerId: character_Amai
    speakerName: Amai
    sentence: Deixe comigo, querida! Posso ser uma simples cozinheira, mas vou defender
      minha filhotinha.
    icon: {fileID: 21300000, guid: 490294f374b60b34881d28e9364b1608, type: 3}
    audio: {fileID: 0}
  - speakerId: character_Niara
    speakerName: Niara
    sentence: "N\xE3o precisa, m\xE3e. Mas agrade\xE7o que esteja disposta a brigar
      com uma general por mim. Ali\xE1s, quer ajuda na cozinha da vila?"
    icon: {fileID: 21300000, guid: c6485e1d61aedaf4a9475bf958b1f17a, type: 3}
    audio: {fileID: 0}
  - speakerId: character_Amai
    speakerName: Amai
    sentence: "Vou aceitar a ajuda, ent\xE3o! Pode por favor ir ver com a Adumi sobre
      um tempero que eu pedi para ela? \xC9 para o almo\xE7o de hoje."
    icon: {fileID: 21300000, guid: 490294f374b60b34881d28e9364b1608, type: 3}
    audio: {fileID: 0}
  - speakerId: character_Amai
    speakerName: Amai
    sentence: E por favor, diga a ela que eu achei o seu novo vestido amarelo muito
      lindo.
    icon: {fileID: 21300000, guid: 490294f374b60b34881d28e9364b1608, type: 3}
    audio: {fileID: 0}
  - speakerId: character_Niara
    speakerName: Niara
    sentence: "Tudo bem, m\xE3e. Vou procur\xE1-la e pedir o condimento."
    icon: {fileID: 21300000, guid: c6485e1d61aedaf4a9475bf958b1f17a, type: 3}
    audio: {fileID: 0}
